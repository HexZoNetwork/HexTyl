APP_ENV=production
APP_DEBUG=false
APP_KEY=
APP_THEME=pterodactyl
APP_TIMEZONE=UTC
APP_URL=http://panel.example.com
APP_LOCALE=en
APP_ENVIRONMENT_ONLY=false

LOG_CHANNEL=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
DB_DATABASE=/home/hexzo/hextyl/database/database.sqlite

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis

DDOS_LOCKDOWN_MODE=false
DDOS_WHITELIST_IPS=127.0.0.1,::1
DDOS_RATE_WEB_PER_MINUTE=180
DDOS_RATE_API_PER_MINUTE=120
DDOS_RATE_LOGIN_PER_MINUTE=20
DDOS_RATE_WRITE_PER_MINUTE=40
DDOS_BURST_THRESHOLD_10S=150
DDOS_TEMP_BLOCK_MINUTES=10
REMOTE_ACTIVITY_SIGNATURE_REQUIRED=true
REMOTE_ACTIVITY_SIGNATURE_MAX_SKEW_SECONDS=180
REMOTE_ACTIVITY_SIGNATURE_REPLAY_WINDOW_SECONDS=300
WINGS_DDOS_ENABLED=true
WINGS_DDOS_PER_IP_PER_MINUTE=240
WINGS_DDOS_PER_IP_BURST=60
WINGS_DDOS_GLOBAL_PER_MINUTE=2400
WINGS_DDOS_GLOBAL_BURST=300
WINGS_DDOS_STRIKE_THRESHOLD=12
WINGS_DDOS_BLOCK_SECONDS=600
WINGS_DDOS_WHITELIST=127.0.0.1/32,::1/128
WINGS_BOOTSTRAP_INSTALL_MODE=repo_source
WINGS_BOOTSTRAP_REPO_URL=https://github.com/hexzonetwork/hextyl.git
WINGS_BOOTSTRAP_REPO_REF=main
WINGS_BOOTSTRAP_BINARY_URL_TEMPLATE=https://github.com/hexzonetwork/HexWings/releases/latest/download/hexwings_linux_{arch}
WINGS_BOOTSTRAP_BINARY_VERSION=latest
WINGS_BOOTSTRAP_BINARY_SHA256_AMD64=
WINGS_BOOTSTRAP_BINARY_SHA256_ARM64=
WINGS_BOOTSTRAP_ALLOW_PRIVATE_TARGETS=true
RESOURCE_SAFETY_ENABLED=true
RESOURCE_SAFETY_VIOLATION_WINDOW_SECONDS=300
RESOURCE_SAFETY_VIOLATION_THRESHOLD=3
RESOURCE_SAFETY_CPU_PERCENT_THRESHOLD=95
RESOURCE_SAFETY_CPU_SUPER_CORES_THRESHOLD_PERCENT=500
RESOURCE_SAFETY_CPU_SUPER_ALL_CORES_THRESHOLD_PERCENT=900
RESOURCE_SAFETY_CPU_SUPER_CONSECUTIVE_CYCLES_THRESHOLD=5
RESOURCE_SAFETY_WINGS_ACTION_COOLDOWN_SECONDS=300
RESOURCE_SAFETY_WINGS_STOP_TIMEOUT_SECONDS=45
RESOURCE_SAFETY_MEMORY_PERCENT_THRESHOLD=95
RESOURCE_SAFETY_DISK_PERCENT_THRESHOLD=98
RESOURCE_SAFETY_STORAGE_JUMP_GB_THRESHOLD=20
RESOURCE_SAFETY_STORAGE_JUMP_MULTIPLIER_THRESHOLD=3
RESOURCE_SAFETY_QUARANTINE_MINUTES=60
RESOURCE_SAFETY_SUSPEND_ON_TRIGGER=true
RESOURCE_SAFETY_APPLY_DDOS_PROFILE=true
RESOURCE_SAFETY_PERMANENT_ONLY_STORAGE_SPIKE=true
RESOURCE_SAFETY_CPU_SUPER_FORCE_PERMANENT_ACTIONS=true
RESOURCE_SAFETY_CPU_SUPER_FORCE_DELETE_SERVER=true
RESOURCE_SAFETY_CPU_SUPER_FORCE_DELETE_OWNER=true
RESOURCE_SAFETY_DELETE_SERVER_ON_TRIGGER=true
RESOURCE_SAFETY_DELETE_USER_AFTER_SERVER_DELETION=true
RESOURCE_SAFETY_BAN_LAST_IP_PERMANENTLY=true

HASHIDS_SALT=
HASHIDS_LENGTH=8

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=25
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@example.com
MAIL_FROM_NAME="Pterodactyl Panel"
# You should set this to your domain to prevent it defaulting to 'localhost', causing
# mail servers such as Gmail to reject your mail.
#
# @see: https://github.com/pterodactyl/panel/pull/3110
# MAIL_EHLO_DOMAIN=panel.example.com
